<odoo>


    <record id="action_high_low_invoice_custom" model="ir.actions.report">
        <field name="name">High/Low Value Invoice CS</field>
        <field name="model">sale.order</field>
        <field name="report_type">qweb-pdf</field>
        <field name="report_name">cs_customs_reports.report_saleorder_copy_1_custom_cs</field>
        <field name="report_file">cs_customs_reports.report_saleorder_copy_1_custom_cs</field>
        <field name="print_report_name">
            (object.state in ('draft', 'sent') and 'Quotation - %s' % (object.name)) or 'Order - %s' % (object.name)
        </field>
        <field name="binding_model_id" ref="sale.model_sale_order"/>
        <field name="binding_type">report</field>
    </record>

