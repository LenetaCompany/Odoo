    <!-- Inventory Report Template -->
    <template id="inventory_report_template">
        <t t-call="web.html_container">
<!--            <t t-call="item_inventory_report.external_custom_layout_boxed"/>-->

            <div class="row mt-2">
                <div class="col-3">
                    <img t-if="env.company.logo" t-att-src="image_data_uri(env.company.logo)" alt="Logo"
                         style="max-height:100px; max-width:90%; width:auto"/>
                </div>
                <div class="col-9" style="font-family:Calibri; font-size:18px; margin-top:20px;">
                    <strong>Item Inventory Report for the period from
                        <span t-esc="start_date.strftime('%m/%d/%Y')"/>
                        to
                        <span t-esc="end_date.strftime('%m/%d/%Y')"/>
                    </strong>
                </div>
            </div>

            <br/>

            <div class="page" style="font-family:Calibri; font-size:12px; page-break-after:auto;">
                <style>
                    table { width: 100%; border-collapse: collapse; }
                    th, td { padding: 4px; border: 1px solid #ddd; font-size: 12px; }
                    tr { page-break-inside: avoid; }
                    thead { display: table-header-group; }  /* Repeat header on new page */
                    tfoot { display: table-footer-group; }
                </style>

                <table class="table table-sm table-striped">
                    <thead>
                        <tr style="background-color:#D3D3D3;">
                            <th>Product Code</th>
                            <th>Available</th>
                            <th>W 01</th>
                            <th>W 02</th>
                            <th>Qty Sold Last 12 Mos</th>
                            <th>Avg Use Per Mo</th>
                            <th>Months of Stock</th>
                        </tr>
                    </thead>
                    <tbody>
    <!-- row counter -->
    <t t-set="row_index" t-value="0"/>

    <tr t-foreach="docs" t-as="item">
        <t t-set="row_index" t-value="row_index + 1"/>

        <!-- alternate background -->
        <t t-set="bg_color" t-value="'#F2F2F2' if row_index % 2 == 0 else '#FFFFFF'"/>

        <!-- Yellow rows for low stock -->
        <t t-if="item['months_of_stock'] &lt;= 6 and item['months_of_stock'] &gt; 0">
            <td style="background-color:#FFFF00;">
                <t t-esc="item.get('default_code') or '-'"/>
            </td>
            <td class="text-center" style="background-color:#FFFF00;" t-esc="item['available_qty']"/>
            <td class="text-center" style="background-color:#FFFF00;" t-esc="item['warehouse1_qty']"/>
            <td class="text-center" style="background-color:#FFFF00;" t-esc="item['warehouse2_qty']"/>
            <td class="text-center" style="background-color:#FFFF00;" t-esc="item['qty_sold_last_12_mo']"/>
            <td class="text-center" style="background-color:#FFFF00;" t-esc="item['avg_use_per_month']"/>
            <td class="text-center" style="background-color:#FFFF00;" t-esc="item['months_of_stock']"/>
        </t>

        <!-- Normal alternate rows -->
        <t t-else="">
            <td t-att-style="'background-color:%s' % bg_color">
                <t t-esc="item.get('default_code') or '-'"/>
            </td>
            <td class="text-center" t-att-style="'background-color:%s' % bg_color" t-esc="item['available_qty']"/>
            <td class="text-center" t-att-style="'background-color:%s' % bg_color" t-esc="item['warehouse1_qty']"/>
            <td class="text-center" t-att-style="'background-color:%s' % bg_color" t-esc="item['warehouse2_qty']"/>
            <td class="text-center" t-att-style="'background-color:%s' % bg_color" t-esc="item['qty_sold_last_12_mo']"/>
            <td class="text-center" t-att-style="'background-color:%s' % bg_color" t-esc="item['avg_use_per_month']"/>

            <td class="text-center" t-att-style="'background-color:%s' % bg_color">
                <t t-if="item['months_of_stock'] == 0
                    and item['available_qty'] == 0
                    and item['warehouse1_qty'] == 0
                    and item['warehouse2_qty'] == 0
                    and item['qty_sold_last_12_mo'] == 0
                    and item['avg_use_per_month'] == 0">
                    N/A
                </t>
                <t t-else="" t-esc="item['months_of_stock']"/>
            </td>
        </t>

    </tr>
</tbody>



